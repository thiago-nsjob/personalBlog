<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Meu Aplicativo ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/bundleBlog")
</head>
<body>
    <header id="headerContent" class="headerContent"> header</header>
    <div id="maincontent" class="mainContent">
        <div id="navBar" class="navBar">
            <div id="navBarContent" class="navBarContent">
                    <ul class="navbar navbar-dark flex-column">
                        <li class="nav-item">About</li>
                        <li class="nav-item">Articles</li>
                        <li class="nav-item">Snippets</li>
                    </ul>
            </div>
            <footer id="navBarFooter" class="navBarFooter">@DateTime.Now</footer>
        </div>
        <div id="articleContainer" class="articleContainer">
            @RenderBody()
        </div>

    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
